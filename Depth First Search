#include<stdio.h>
void dfs(int v);
int n,i,j,a[10][10],vis[10],count=0;
void main(){

    printf("Enter the number of nodes:");
    scanf("%d",&n);
    printf("Enter elements of adjacency matrix:");
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            scanf("%d",&a[i][j]);
        }
        vis[i]=0;
    }
    for(i=1;i<=n;i++){
        if(vis[i]==0){
            dfs(i);
        }
    }
    for(i=1;i<=n;i++){
        if(vis[i]==1){
            count++;
        }
    }
    if(count==n){
        printf("Graph is connected");
    }
    else{
        printf("Graph is not connected");
    }
}
void dfs(int v){
    vis[v]=1;
    printf("%d ",v);
    for(i=1;i<=n;i++){
        if(a[v][i]==1 && vis[i]==0){
            dfs(i);
        }
    }
}
